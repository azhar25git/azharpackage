{
    "name": "azhar25git/azhar-package",
    "description": "Laravel package",
    "keywords": [
        "azhar25git",
        "azhar-package"
    ],
    "homepage": "https://github.com/azhar25git/azhar-package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Azhar Uddin",
            "email": "getazharuddin@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Azhar25git\\AzharPackage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Azhar25git\\AzharPackage\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Azhar25git\\AzharPackage\\AzharPackageServiceProvider"
            ],
            "aliases": {
                "AzharPackage": "Azhar25git\\AzharPackage\\AzharPackageFacade"
            }
        }
    }
}
